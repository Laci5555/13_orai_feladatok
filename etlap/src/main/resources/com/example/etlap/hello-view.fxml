<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="5.0" style="-fx-background-image: url(https://www.pet4you.hu/img/big/15006357333312_1.jpg?1541453100); -fx-background-repeat: no-repeat; -fx-background-size: cover;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.etlap.HelloController">
   <children>
      <HBox spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onElozoClick" prefWidth="100.0" text="Előző" />
            <Label fx:id="etelsor" prefWidth="300.0" text="?" textFill="WHITE" />
            <Button mnemonicParsing="false" onAction="#onKovetkezoClick" prefWidth="100.0" text="Következő" />
         </children>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onDragaClick" prefWidth="100.0" text="Drága" />
            <Label fx:id="Draga" prefWidth="300.0" text="?" textFill="WHITE" />
         </children>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onAtlagClick" prefWidth="100.0" text="Átlag" />
            <Label fx:id="lbatlag" prefWidth="300.0" text="?" textFill="WHITE" />
         </children>
      </HBox>
      <HBox spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onSertesClick" prefWidth="100.0" text="Sertés" />
            <Label fx:id="lbsertes" prefWidth="300.0" text="?" textFill="WHITE" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
