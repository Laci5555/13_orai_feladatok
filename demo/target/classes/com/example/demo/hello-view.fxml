<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <ImageView fitHeight="96.0" fitWidth="96.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@diak.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#onMindenkiClick" prefWidth="96.0" text="Mindenki" />
            <Button mnemonicParsing="false" onAction="#onSandorokClick" prefWidth="96.0" text="Sándorok" />
            <Button mnemonicParsing="false" onAction="#onKecskemetiClick" prefWidth="96.0" text="Kecskeméti" />
            <Button mnemonicParsing="false" onAction="#on1996Click" prefWidth="96.0" text="1996" />
            <Button mnemonicParsing="false" onAction="#on10AClick" prefWidth="96.0" text="10/A" />
         </children>
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </VBox>
      <VBox alignment="TOP_RIGHT" spacing="10.0">
         <children>
            <ListView fx:id="lista" prefHeight="270.0" prefWidth="400.0" />
            <Button mnemonicParsing="false" onAction="#onListaMenteseClick" prefWidth="96.0" text="Lista mentése" />
         </children>
         <HBox.margin>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </VBox>
   </children>
</HBox>
